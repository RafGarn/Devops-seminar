pipeline {
    agent any

    stages {
        stage('Pre-build stg') {
            steps {
              
              sh 'docker build --target Pre-build'
            }
        }
        stage('Build') {
            steps {
              
              sh 'docker build --target Build'
            }
        }
        stage('Test') {
            steps {
              
              sh 'docker build --target Test'
            }
        }
        stage('Security') {
            steps {
              sh 'docker build --target Security'
            }
        }
        stage('Back-end') {
            steps {
              sh 'docker build --target Back-end'
            }
        }
        stage('Front-end') {
            steps {
              sh 'docker build --target Front-end'
            }
        }
        stage('Deploy') {
            steps {
              sh 'docker build --target Deploy'
            }
        }
        stage('Post') {
            steps {
              sh 'docker build --target Post'
              
            }
        }
    }
}
